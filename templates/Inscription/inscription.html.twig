{% extends 'layout.html.twig' %}
{% block content %}
    <div class="container">
        <div class="row">
            <form method="post">
                <fieldset>
                    <legend>Inscription</legend>
                    <div class="nom">
                        {{ form_row(form.nom) }}
                    </div>
                    <div class="prenom">
                        {{ form_row(form.prenom) }}
                    </div>
                    <div class="mail">
                        {{ form_row(form.mail) }}
                    </div>
                    <div class="password">
                        {{ form_row(form.password) }}
                    </div>

                    {{ form_end(form) }}

                    {#{{ form(form) }}#}
                </fieldset>
            </form>
        </div>
    </div>

{% endblock %}